<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pressure.View"
        xmlns:VM="clr-namespace:Pressure.VM"
        xmlns:Pressure="clr-namespace:Pressure" x:Class="Pressure.View.DBWindow"
        mc:Ignorable="d"
        Title="DBWindow" Height="536" Width="759">

    <Window.DataContext>
        <VM:DatabaseVM/>
    </Window.DataContext>

    <Grid Margin="0,0,0,-6">
        <DataGrid ItemsSource="{Binding DataGridDB}" Margin="0,53,0,0"/>
    </Grid>
</Window>
